<UserControl x:Name="userControl" x:Class="VULauncher.Views.Controls.Consoles.VuConsoleControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:vc="clr-namespace:VULauncher.Views.Controls.Consoles"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800" Background="Black">
    <Grid>
        <vc:ConsoleOutput x:Name="ConsoleOutput"
                    VerticalScrollBarVisibility="Auto"
                    IsReadOnly="True"
                    Foreground="White" FontFamily="Consolas" Background="Black"
                    ItemsSource="{Binding Path=CustomInlines, Mode=OneWay}">
        </vc:ConsoleOutput>
    </Grid>
</UserControl>
